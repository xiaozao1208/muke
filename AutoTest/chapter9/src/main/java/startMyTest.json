[
  {
    "description":"这是返回cookies信息的get请求",
    "request":{
      "uri":"/getCookies",
      "method":"get"
    },
    "response":{
      "cookies":{
        "login":"true"
      },
      "text":"恭喜你获取cookies成功",
      "headers":{
        "Content-Type":"text/html; charset=gbk"
      }
    }

  },
  {
    "description":"这是一个带cookies信息的get请求",
    "request":{
      "uri":"/get/withcookies",
      "method":"get",
      "cookies":{
        "login":"true"
      }
    },
    "response":{
      "text":"这个是需要cookies信息才能访问的get请求",
      "headers":{
        "Content-Type":"text/html;charset=gbk"
      }

    }

  },
  {
    "description":"这是一个需要cookies信息才能访问的post请求",
    "request":{
      "uri":"/post/withcookies",
      "method":"post",
      "cookies":{
        "login":"true"
      },
      "json":{
        "name":"lijuan",
        "age":"18"
      }
    },
    "response":{
      "headers":{
        "Content-Type":"text/html;charset=gbk"
      },
      "status":"200",
      "json":{
        "lijuan":"success",
        "status":"1"
      }
    }
  }

]